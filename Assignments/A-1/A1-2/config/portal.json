{
   "version":1,
   "request":{
      "url":"https://api.collegefootballdata.com/player/portal",
      "method":"GET",
      "authorization":{
         "type":"Bearer"
      },
      "params":{
         "year":true
      }
   },
   "response":{
      "ctype":"application/json",
      "tablePath":"$.[*]",
      "schema":{
         "season":{
            "target":"$.season",
            "type":"int"
         },
         "firstName":{
            "target":"$.firstName",
            "type":"string"
         },
         "lastName":{
            "target":"$.lastName",
            "type":"string"
         },
         "position":{
            "target":"$.position",
            "type":"string"
         },
         "origin":{
            "target":"$.origin",
            "type":"string"
         },
         "destination":{
            "target":"$.destination",
            "type":"string"
         },
         "transferDate":{
            "target":"$.transferDate",
            "type":"string"
         },
         "ratings":{
            "target":"$.ratings",
            "type":"int"
         },
         "stars":{
            "target":"$.stars",
            "type":"int"
         },
         "eligibility":{
            "target":"$.eligibility",
            "type":"string"
         }
      },
         "orient":"records"
      }
   }